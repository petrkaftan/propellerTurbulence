FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      yPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    innerCylinder
    {
        type            calculated;
        value           uniform 0;
    }
    innerCylinder_slave
    {
        type            calculated;
        value           uniform 0;
    }
    propellerTip
    {
        type            calculated;
        value           uniform 0;
    }
    nonConformalCyclic_on_innerCylinder
    {
        type            nonConformalCyclic;
    }
    nonConformalCyclic_on_innerCylinder_slave
    {
        type            nonConformalCyclic;
    }
    nonConformalError_on_innerCylinder
    {
        type            nonConformalError;
    }
    nonConformalError_on_innerCylinder_slave
    {
        type            nonConformalError;
    }
}


