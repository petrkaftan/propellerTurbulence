FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      MRFProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

MRF1
{
    /* cellZone    rotor; */
    cellZone    innerCylinder;
    active      yes;

    // Fixed patches (by default they 'move' with the MRF zone)
    nonRotatingPatches (AMI1 AMI2);

    origin    (0 0 0);
    axis      (0 1 0);
    omega     733.03;
}

