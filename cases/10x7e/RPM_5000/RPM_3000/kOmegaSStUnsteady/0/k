FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.06;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
	
	walls
	{
		type            zeroGradient;
	}

    propellerTip
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    innerCylinder
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    innerCylinder_slave
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
